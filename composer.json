{
	"name": "dryven/faviconator",
	"type": "statamic-addon",
	"version": "1.1.0",
	"description": "Adding favicons to your site the simple way.",
	"keywords": [
		"favicon",
		"favicon generator"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "dryven",
			"email": "hallo@dryven.at",
			"homepage": "https://dryven.at/"
		}
	],
	"support": {
		"email": "support@dryven.at"
	},
	"require": {
		"php": "^7.3 || ^8.0",
		"statamic/cms": "^3.0",
		"ext-gd": "*"
	},
	"autoload": {
		"psr-4": {
			"Dryven\\Faviconator\\": "src"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Dryven\\Faviconator\\ServiceProvider"
			]
		},
		"statamic": {
			"name": "Faviconator",
			"description": "Adding favicons to your site the simple way.",
			"version": "1.1.0"
		}
	},
	"config": {
		"allow-plugins": {
			"pixelfear/composer-dist-plugin": true
		}
	}
}
